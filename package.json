{
  "name": "tim-stoddard-website",
  "version": "1.0.0",
  "description": "Tim Stoddard's Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack:dev",
    "start:prod": "npm run build:s3 && node start-server.js",
    "webpack:dev": "webpack --config webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --open",
    "webpack:prod": "webpack -p --config webpack/webpack.prod.js",
    "build:s3": "npm run clean && npm run webpack:prod",
    "s3": "npm run build:s3 && ./upload.sh && npm run purge-cache",
    "clean": "rm -rf dist index.html website.zip",
    "lint": "tslint src/**/*.ts --fix",
    "purge-cache": "node utils/cloudflare-purge-cache.js"
  },
  "author": "Tim Stoddard",
  "repository": {
    "url": "https://github.com/timstoddard/website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "prop-types": "^15.7.2",
    "r2": "^2.0.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "underscore": "^1.9.2",
    "webpack": "^4.35.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/underscore": "^1.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "http-server": "^0.11.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
