{
  "name": "tim-stoddard-website",
  "version": "1.0.0",
  "description": "Tim Stoddard's Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack:dev",
    "test": "jest",
    "webpack:dev": "webpack serve -c webpack/webpack.dev.js --open",
    "webpack:prod": "webpack -c webpack/webpack.prod.js",
    "build:s3": "npm run clean && npm run webpack:prod",
    "s3": "npm run build:s3 && ./utils/upload/dev.sh && ./utils/upload/prod.sh && npm run purge-cache",
    "s3:prod": "npm run build:s3 && ./utils/upload/prod.sh && npm run purge-cache",
    "s3:dev": "npm run build:s3 && ./utils/upload/dev.sh && npm run purge-cache",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.tsx? --fix",
    "purge-cache": "node utils/cloudflare/cloudflare-purge-cache.js",
    "setup": "./utils/set-up-node-modules.sh"
  },
  "author": "Tim Stoddard",
  "repository": {
    "url": "https://github.com/timstoddard/website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.3",
    "r2": "^2.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "underscore": "^1.13.1",
    "webpack": "^5.61.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/underscore": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "css-loader": "^6.5.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.1.0",
    "eslint-webpack-plugin": "^3.1.0",
    "favicons-webpack-plugin": "^5.0.2",
    "fork-ts-checker-notifier-webpack-plugin": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
