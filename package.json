{
  "name": "tim-stoddard-website",
  "version": "1.0.0",
  "description": "Tim Stoddard's Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack:dev",
    "start:prod": "npm run build:s3 && node start-server.js",
    "webpack:dev": "webpack --config webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --open",
    "webpack:prod": "webpack -p --config webpack/webpack.prod.js",
    "build:s3": "npm run clean && npm run webpack:prod",
    "clean": "rm -rf dist index.html website.zip",
    "lint": "eslint --ext .jsx,.js --ignore-pattern '/dist/*' --fix ."
  },
  "author": "Tim Stoddard",
  "repository": {
    "url": "https://github.com/timstoddard/website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "axios": "^0.18.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.31.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
