{
  "name": "tim-stoddard-website",
  "version": "1.0.0",
  "description": "Tim Stoddard's Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack:dev",
    "webpack:dev": "webpack serve -c webpack/webpack.dev.js --open",
    "webpack:prod": "webpack -c webpack/webpack.prod.js",
    "build:s3": "npm run clean && npm run webpack:prod",
    "s3": "npm run build:s3 && ./utils/upload/prod.sh && ./utils/upload/dev.sh && npm run purge-cache",
    "s3:prod": "npm run build:s3 && ./utils/upload/prod.sh",
    "s3:dev": "npm run build:s3 && ./utils/upload/dev.sh",
    "clean": "rm -rf dist",
    "lint": "tslint src/**/*.tsx? --fix",
    "purge-cache": "node utils/cloudflare/cloudflare-purge-cache.js",
    "setup": "./utils/set-up-node-modules.sh"
  },
  "author": "Tim Stoddard",
  "repository": {
    "url": "https://github.com/timstoddard/website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.3",
    "r2": "^2.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "underscore": "^1.12.0",
    "webpack": "^5.12.2",
    "webpack-merge": "^5.7.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/underscore": "^1.10.24",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^5.0.1",
    "css-modules-typescript-loader": "^4.0.1",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
