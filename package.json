{
  "name": "tim-stoddard-website",
  "version": "1.0.0",
  "description": "Tim Stoddard's Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack:dev",
    "webpack:dev": "webpack-dev-server --config webpack/webpack.dev.js --open",
    "webpack:prod": "webpack -p --config webpack/webpack.prod.js",
    "build:s3": "npm run clean && npm run webpack:prod",
    "s3": "npm run build:s3 && ./utils/upload/prod.sh && ./utils/upload/dev.sh && npm run purge-cache",
    "s3:prod": "npm run build:s3 && ./utils/upload/prod.sh",
    "s3:dev": "npm run build:s3 && ./utils/upload/dev.sh",
    "clean": "rm -rf dist",
    "lint": "tslint src/**/*.tsx? --fix",
    "purge-cache": "node utils/cloudflare/cloudflare-purge-cache.js"
  },
  "author": "Tim Stoddard",
  "repository": {
    "url": "https://github.com/timstoddard/website"
  },
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.5",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "html-webpack-plugin": "^4.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "r2": "^2.0.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
